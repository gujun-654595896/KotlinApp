<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gujun.media">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.KotlinApplication"
        android:allowBackup="true"
        android:icon="@mipmap/media_ic_launcher"
        android:label="@string/media_name"
        android:roundIcon="@mipmap/media_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MediaTheme">
        <activity android:name=".MediaTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>